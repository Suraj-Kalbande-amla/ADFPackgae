{
	"name": "dataflow3",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "AzureSqlDatabase",
						"type": "DatasetReference"
					},
					"name": "SQLserver"
				}
			],
			"sinks": [
				{
					"linkedService": {
						"referenceName": "amalelatic",
						"type": "LinkedServiceReference"
					},
					"name": "Elastic"
				}
			],
			"transformations": [
				{
					"name": "alterRow1"
				}
			],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     query: 'SELECT TOp 100  * FROM ZnodePimProduct \\n',",
				"     format: 'query') ~> SQLserver",
				"SQLserver alterRow() ~> alterRow1",
				"alterRow1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     insertHttpMethod: 'POST',",
				"     requestFormat: ['type' -> 'json'],",
				"     format: 'rest',",
				"     timeout: 30,",
				"     headers: ['Content-Type' -> 'application/json', 'Authorization' -> 'ApiKey VRlpFBdJQeWrLbrvER3hNw'],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> Elastic"
			]
		}
	}
}