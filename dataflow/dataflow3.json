{
	"name": "dataflow3",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "AzureSqlDatabase",
						"type": "DatasetReference"
					},
					"name": "SQLserver"
				}
			],
			"sinks": [
				{
					"linkedService": {
						"referenceName": "amalelatic",
						"type": "LinkedServiceReference"
					},
					"name": "elasticdata"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> SQLserver",
				"SQLserver sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     insertHttpMethod: 'POST',",
				"     requestFormat: ['type' -> 'json'],",
				"     format: 'rest',",
				"     timeout: 30,",
				"     requestInterval: 0,",
				"     headers: ['Content-Type' -> 'application/json', 'Authorization' -> 'ApiKey VRlpFBdJQeWrLbrvER3hNw']) ~> elasticdata"
			]
		}
	}
}