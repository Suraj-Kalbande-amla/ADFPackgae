{
	"name": "dataflow3",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "AzureSqlDatabase",
						"type": "DatasetReference"
					},
					"name": "SQLserver"
				}
			],
			"sinks": [
				{
					"linkedService": {
						"referenceName": "amalelatic",
						"type": "LinkedServiceReference"
					},
					"name": "elasticdata"
				}
			],
			"transformations": [
				{
					"name": "AlterRow1"
				}
			],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     query: 'SELECT TOP 100 PimProductId, (SELECT * FROM ZnodePimProduct r \\nWHERE r.PimProductId = m.PimProductId \\nFOR JSON PATH \\n) JSONValue\\nFROM ZnodePimProduct m \\n',",
				"     format: 'query') ~> SQLserver",
				"SQLserver alterRow() ~> AlterRow1",
				"AlterRow1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     deletable:true,",
				"     insertable:true,",
				"     updateable:true,",
				"     upsertable:true,",
				"     insertHttpMethod: 'PATCH',",
				"     deleteHttpMethod: 'DELETE',",
				"     upsertHttpMethod: 'PUT',",
				"     updateHttpMethod: 'PATCH',",
				"     requestFormat: ['type' -> 'json'],",
				"     format: 'rest',",
				"     timeout: 30,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> elasticdata"
			]
		}
	}
}