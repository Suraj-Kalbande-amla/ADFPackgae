{
	"name": "dataflow3",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "AzureSqlDatabase",
						"type": "DatasetReference"
					},
					"name": "SQLserver"
				},
				{
					"linkedService": {
						"referenceName": "amalelatic",
						"type": "LinkedServiceReference"
					},
					"name": "ElasticSearch"
				}
			],
			"sinks": [],
			"transformations": [],
			"scriptLines": [
				"parameters{",
				"     jsonvalue as string",
				"}",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     query: 'SELECT TOP 100 PimProductId, (SELECT * FROM ZnodePimProduct r \\nWHERE r.PimProductId = m.PimProductId \\nFOR JSON PATH \\n) JSONValue\\nFROM ZnodePimProduct m \\n',",
				"     format: 'query') ~> SQLserver",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'rest',",
				"     timeout: 30,",
				"     httpMethod: 'POST',",
				"     body: ($jsonvalue),",
				"     paginationRules: ['supportRFC5988' -> 'true'],",
				"     responseFormat: ['type' -> 'json', 'documentForm' -> 'documentPerLine']) ~> ElasticSearch"
			]
		}
	}
}